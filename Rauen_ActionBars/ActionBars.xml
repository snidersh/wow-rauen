<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/C:\Projects\WoW\Bin\Interface\FrameXML\UI.xsd">

	<Script file="_Version.lua"/>
	<Script file="ActionBars.lua"/>
	<Script file="PartyFrame.lua"/>

	<Frame name="BarFrame" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				Bars_OnLoad();
			</OnLoad>
			<OnEvent>
				Bars_OnEvent(event);	
			</OnEvent>
		</Scripts>
	</Frame>

	<CheckButton name="BarButton" inherits="ActionBarButtonTemplate" virtual="true">
		<Scripts>		
			<OnEnter>
				GameTooltip:SetOwner(this, "ANCHOR_RIGHT");
				if ( GameTooltip:SetAction(ActionButton_GetPagedID(this)) ) then
					this.updateTooltip = TOOLTIP_UPDATE_TIME;
				else
					this.updateTooltip = nil;
				end
			</OnEnter>
		</Scripts>
	</CheckButton>
	
