<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ C:ProjectsWoWBinInterfaceFrameXMLUI.xsd">

	<Script file="_Version.lua"/>
	<Script file="Karma.lua"/>

	<Frame name="Frame_Karma" hidden="false">
		<Scripts>
			<OnLoad>
				Karma_OnLoad();
			</OnLoad>
			<OnEvent>
				Karma_OnEvent(event);
			</OnEvent>
		</Scripts>
	</Frame>
	<Frame name="Karma_Tooltip" parent="GameTooltip">
		<Scripts>
			<OnShow>
				Karma_OnShow();
			</OnShow>
		</Scripts>
	</Frame>

</Ui>